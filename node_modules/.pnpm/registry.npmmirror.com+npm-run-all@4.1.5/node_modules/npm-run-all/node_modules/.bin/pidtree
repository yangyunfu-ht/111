#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/bin/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/bin/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.3.1/node_modules:/Users/yyg/Documents/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+pidtree@0.3.1/node_modules/pidtree/bin/pidtree.js" "$@"
fi
